{
    "name": "web-app-boilerplate/image-service",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Vasilyev",
            "email": "alex@onlamp.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/flex": "^2.2",
        "guzzlehttp/guzzle": "^6.3",
        "fabpot/goutte": "^3.2",
        "php-amqplib/rabbitmq-bundle": "^2.11",
        "symfony/serializer": "^5.4",
        "symfony/property-access": "^5.4"
    },
    "require-dev": {
        "symfony/dotenv": "^5.4",
        "symfony/yaml": "^5.4",
        "symfony/maker-bundle": "^1",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
    ,
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
